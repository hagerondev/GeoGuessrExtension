{
  "name": "GeoGuessrExtension",
  "description": "Created by hageron",
  "version": "1.4.1",
  "manifest_version": 3,
  "action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.geoguessr.com/challenge/*"],
      "js": [
        "ws.js",
        "ad.js",
        "Ready.js",
        "Ranking.js",
        "capture.js"
      ]
    },
    {
      "matches": ["https://www.geoguessr.com/results/*"],
      "js": [
        "results.js"
      ]
    }
  ],
  "permissions": []
}